[default]

######## model
model=MF

########device
gpu_id=0
seed=2020

######## dataset
data.name=ml-100k
data.path=dataset

data.format=UIRT

data.convert.separator='\t'

######## pre-processing/filtering/spilt
process.user_min=0
process.item_min=0

process.splitter=ratio

process.ratio=0.8
process.by_time=False

######## evaluating
eval.metric=["Precision", "Recall", "NDCG", "MAP", "MRR"]
eval.topk=[10, 20]
eval.group_view=None
eval.candidate_neg=0
eval.test_batch_size=128
